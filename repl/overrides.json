{
  "@jupyterlab/apputils-extension:themes": {
    "theme-scrollbars": true,
    "theme": "JupyterLab Night"
  },
  "@jupyterlab/console-extension:tracker": {
    "isEnabled": true,
    "interactionMode": "terminal"
  },
  "@jupyterlab/shortcuts-extension:plugin": {
    "shortcuts": [
      {
        "command": "runmenu:run",
        "keys": [
          "Enter"
        ],
        "selector": "body"
      }
    ]
  }
}
